generator client {
  provider = "prisma-client-js"
  output   = "./generated/gameDataClient"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL2")
}

model Item {
  id         Int    @id @default(autoincrement())
  item_code  Int    @unique
  item_name  String
  health     Int
  power      Int
  item_price Int
}
